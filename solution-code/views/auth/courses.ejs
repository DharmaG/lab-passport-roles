<div class="row">
  <div class="col-md-8 col-md-offset-2">
    <h1>Ironhack available courses</h4>
  </div>
</div>
<div class="row">
  <div class="col-sm-6 col-sm-offset-3">
    <ul class="list-group">
      <% courses.forEach((course) =>{ %>
        <li class="list-group-item course-title">
          <%= course.name%>
        </li>
      <% }) %>
    </ul>
  </div>
</div>
